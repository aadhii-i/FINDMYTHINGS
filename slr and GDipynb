      "source": [
        "x = data[\"Age\"].tolist()\n",
        "y = data[\"Cholestrol\"].tolist()\n"
      ]
    },
    {
      "cell_type": "code",
      "execution_count": null,
      "id": "f955b19d-4712-4202-96b4-add63695ee3c",
      "metadata": {
        "id": "f955b19d-4712-4202-96b4-add63695ee3c"
      },
      "outputs": [],
      "source": [
        "n = len(x)"
      ]
    },
    {
      "cell_type": "code",
      "execution_count": null,
      "id": "4545c9c2-6e20-4577-be6b-d217a28fc5d8",
      "metadata": {
        "id": "4545c9c2-6e20-4577-be6b-d217a28fc5d8"
      },
      "outputs": [],
      "source": [
        "sum_x = 0\n",
        "sum_y = 0\n",
        "\n",
        "for i in range(n):\n",
        "    sum_x += x[i]\n",
        "    sum_y += y[i]\n",
        "\n",
        "mean_x = sum_x / n\n",
        "mean_y = sum_y / n\n",
        "\n",
        "num = 0\n",
        "den = 0"
      ]
    },
    {
      "cell_type": "code",
      "execution_count": null,
      "id": "5d797803-b522-4bc1-8fc6-eab613d0f730",
      "metadata": {
        "id": "5d797803-b522-4bc1-8fc6-eab613d0f730",
        "outputId": "512c2244-9fcb-453a-850e-cd8977e05e15"
      },
      "outputs": [
        {
          "name": "stdout",
          "output_type": "stream",
          "text": [
            "Beta0 = 120.0\n",
            "Beta1 = 2.0\n"
          ]
        }
      ],
      "source": [
        "for i in range(n):\n",
        "    num += (x[i] - mean_x) * (y[i] - mean_y)\n",
        "    den += (x[i] - mean_x) ** 2\n",
        "\n",
        "beta1 = num / den\n",
        "beta0 = mean_y - beta1 * mean_x\n",
        "\n",
        "print(\"Beta0 =\", beta0)\n",
        "print(\"Beta1 =\", beta1)"
      ]
    },
    {
      "cell_type": "code",
      "execution_count": null,
      "id": "75524c21-4473-40d8-b547-2726fd78244f",
      "metadata": {
        "id": "75524c21-4473-40d8-b547-2726fd78244f"
      },
      "outputs": [],
      "source": [
        "y_pred = []"
      ]
    },
    {
      "cell_type": "code",
      "execution_count": null,
      "id": "81827d1e-b740-4b70-829f-e174417aebb4",
